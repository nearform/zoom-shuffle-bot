{
  "name": "zoom-shuffle-bot",
  "version": "1.0.0",
  "description": "",
  "main": "chatbot.js",
  "type": "module",
  "scripts": {
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "jest --testPathIgnorePatterns=test/integration",
    "test:integration": "jest --testPathPattern=test/integration --runInBand",
    "start": "npm run db:migrate && node chatbot.js",
    "dev": "nodemon chatbot.js",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "db:up": "docker-compose up -d",
    "db:migrate": "postgrator --config=./postgrator.cjs",
    "db:migrate:rollback": "postgrator 0 --config=./postgrator.cjs",
    "db:reset": "npm run db:populate:rollback && npm run db:migrate:rollback && npm run db:migrate && npm run db:populate",
    "db:populate": "postgrator --config=./postgrator-populate.cjs",
    "db:populate:rollback": "postgrator 0 --config=./postgrator-populate.cjs",
    "db:down": "docker-compose down",
    "db:connect": "docker-compose exec postgres psql -U postgres -d postgres"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "desm": "^1.3.0",
    "dotenv": "^16.0.3",
    "env-schema": "^5.1.0",
    "fast-json-stringify": "^5.4.0",
    "fastify": "^4.9.2",
    "fastify-plugin": "^4.3.0",
    "fastify-postgres": "^3.7.0",
    "fluent-json-schema": "^4.0.0",
    "http-errors": "^2.0.0",
    "node-fetch": "^3.2.10",
    "pg": "^8.8.0",
    "pino-pretty": "^9.1.1",
    "postgrator-cli": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@jest/globals": "^29.3.0",
    "babel-jest": "^29.2.2",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.2.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1"
  }
}
